<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.humorchen.jimu.export.mapper.DownloadTaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.humorchen.jimu.export.bean.DownloadTask">
        <id column="id" property="id"/>
        <result column="account" property="account"/>
        <result column="title" property="title"/>
        <result column="icon" property="icon"/>
        <result column="url" property="url"/>
        <result column="file_size" property="fileSize"/>
        <result column="percent" property="percent"/>
        <result column="state" property="state"/>
        <result column="error" property="error"/>
        <result column="json" property="json"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account , title, icon, url, file_size, percent, state, error, json, create_time, update_time
    </sql>

</mapper>
